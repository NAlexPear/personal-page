{
  "extends": [
    "airbnb/base"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [ 2, { "devDependencies": true } ],
    "import/extensions": 0,
    "indent": ["error", 2, { "MemberExpression": 1 }],
    "no-underscore-dangle": 0,
    "implicit-arrow-linebreak": "off"
  }
}
